<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.justvote.dao.IRegisterDao">
	<!-- 회원가입 -->
	<insert id="registerDao">
		insert into user2(userName,sex,age,major,grade,nickName,userId,userPass)
		values
		(#{param1},#{param2},#{param3},#{param4},#{param5},#{param6},#{param7},#{param8})

	</insert>

	<!-- 로그인 -->
	<select id="loginDao" resultType="com.justvote.dto.RegisterDto">
		select userId,userPass from user2 where userId = #{_userId} and userPass =
		#{_userPass}

	</select>
</mapper>